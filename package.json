  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "lint:fix": "eslint . --cache --cache-location .eslintcache --fix",
    "lint:clear-cache": "rm -f .eslintcache",
    "lint:ci": "eslint . --no-cache",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=basic",
    "test:security": "vitest run tests/security",
    "test:security:ci": "vitest run tests/security --coverage --reporter=basic",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "clean": "rm -rf dist node_modules/.cache .eslintcache",
    "deps:check": "pnpm outdated",
    "deps:update": "pnpm update --latest",
    "ci:check": "npm run lint:ci && npm run typecheck && npm run test:ci && npm run build"
